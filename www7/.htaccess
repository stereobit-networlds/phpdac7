RewriteRule ^([^/]*)/$ /index.php?t=$1 [L]

RewriteRule ^(.*)\.printer$ $1.php [L] 

RewriteRule .* - [E=DEVMD_AUTHORIZATION:%{HTTP:Authorization}]

