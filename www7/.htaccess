RewriteRule ^api/([^/]*)/([^/]*)/$ /api.php?t=api&cat=$1&apikey=$2 [L]
RewriteRule ^api/([^/]*)/$ /api.php?t=api&apikey=$1 [L]

RewriteRule ^product/([^/]*)/([^/]*)/([^/]*)/$ /katalog.php?t=kshow&cat=$1&page=$2&id=$3 [L]
