RewriteRule ^process/([^/]*)/([^/]*)/$ /process.php?t=process&pid=$1&clp=$2 [L]
RewriteRule ^process/([^/]*)/$ /process.php?t=process&pid=$1 [L]
RewriteRule ^process/$ /process.php?t=process [L]

RewriteRule ^p/([^/]*)/([^/]*)/([^/]*)/$ /process_$1.php??t=process&pid=$2&clp=$3 [L]
