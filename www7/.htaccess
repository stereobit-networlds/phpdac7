RewriteRule ^search/([^/]*)/$ /search.php?t=search&input=$1 [L]

RewriteRule ^filter/([^/]*)/([^/]*)/([^/]*)/$ /search.php?t=filter&input=$1&cat=$2&page=$3 [L]
